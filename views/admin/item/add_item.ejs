 <div class="tab-pane fade show" id="add-item" role="tabpanel" aria-labelledby="home-tab">
   <form action="/admin/item" method="POST" enctype="multipart/form-data">
       <div class="form-group">
         <label for="nameBank">Title</label>
         <input type="text" class="form-control" id="nameBank" name="nameBank" placeholder="Enter name bank" required>
       </div>
       <div class="form-group">
         <label for="nomorRekening">Price</label>
         <input type="text" class="form-control" id="nomorRekening" name="nomorRekening"
           placeholder="Enter nomor rekening" required>
       </div>
       <div class="form-group">
         <label for="name">City</label>
         <input type="text" class="form-control" id="name" name="name" placeholder="Enter name" required>
       </div>
       <div class="form-group">
         <label for="name">Category</label>
         <select class="form-control" name="categoryId">
           <option value="">--- Category ---</option>
           <option value="">Ambil dari Database</option>
         </select>
       </div>
       <div class="form-group">
         <label for="image">Image</label>
         <input type="file" class="form-control" id="image" name="image" multiple required>
         <span>Pilih Gambar</span>
       </div>
       <div class="form-group">
         <label for="name">Description</label>
         <textarea name="about" class="form-control"></textarea>
       </div>
     <button type="submit" class="btn btn-primary">Save</button>
   </form>
 </div>